@model List<Pharmacy.Domain.Entities.Category>

<h1>Categories</h1>
<table class="table">
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">Id</th>
			<th scope="col">Name</th>
		</tr>
	</thead>
	<tbody>
		@for(int i = 0; i < Model.Count;i++)
		{
			
		<tr>
				<th scope="row">@(i + 1)</th>
				<td>@Model[i].Id</td>
				<td>@Model[i].Name</td>
		</tr>
		}
	</tbody>
</table>